{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-Wall",
				"-Wextra",
				"-std=c11",
				"-I",
				"${workspaceFolder}/include/",
				"-g",
				"${file}",
				"${workspaceFolder}/src/bsearch_tree.c",
				"-o",
				"${workspaceFolder}/build/bin/main.out"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}